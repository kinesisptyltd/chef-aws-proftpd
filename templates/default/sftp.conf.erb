# Generated by Chef - local changes will be overwritten!

<IfModule mod_sftp.c>
  <VirtualHost 0.0.0.0>
    Port <%= node[:proftpd][:sftp][:port] %>

    SFTPEngine on
    SFTPLog <%= node[:proftpd][:sftp][:log] %>

    SFTPHostKey <%= node[:proftpd][:sftp][:host_rsa_key] %>
    SFTPHostKey <%= node[:proftpd][:sftp][:host_dsa_key] %>

    SFTPCompression delayed
    SFTPAuthMethods <%= node[:proftpd][:sftp][:auth_methods] %>
  </VirtualHost>
</IfModule>
